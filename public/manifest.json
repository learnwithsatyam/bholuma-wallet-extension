{
    "manifest_version": 3,
    "name": "Bholuma Wallet Extension",
    "version": "1.0.0",
    "description": "A Web3 wallet extension for managing your crypto assets.",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}